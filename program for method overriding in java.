class Vehicle
{
void drive()
{
System.out.println("Anusha drives vehicle\n");
}
void repair()
{
System.out.println("vehicle repaired\n");
}
}
class Car extends Vehicle
{
void drive()
{
System.out.println("Anusha drives car\n");
}
}
class main
{
public static void main(String args[] )
{
Car c1=new Car();
c1.drive();
c1.repair();
Vehicle v1=new Vehicle();
v1.drive();
}
}
